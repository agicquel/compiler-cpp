function isNotZero:
read X
%
  nop
%
write X

function isZero:
read X
%
  if X then
    B := nil
  else
    B := (cons nil nil)
  fi
%
write B

function incr:
read X
%
  X := (cons nil X)
%
write X

function decr:
read X
%
  X := (tl X)
%
write X

function greater:
read X,Y
%
  if X =? Y then
    R := nil
  else
    while not X =? nil and not Y =? nil do
      X := (tl X) ;
      Y := (tl Y)
    od ;
    if X =? nil then
      R := nil
    else
      R := (cons nil nil)
    fi
  fi
%
write R

